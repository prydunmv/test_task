<h3>arrticles</h3>
  <% @articles.each do |article| %>
  <table>
    <tr>
      <td><%= article.title %></td>
      <td><%= article.description %></td>
      <td><%= link_to 'show', article_path(article) %></td>
    </tr>
  </table>
  <% end %>
  <p><%= link_to 'new article', new_article_path %></p>

